<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="kematjaya_link_index" path="/link" methods="GET POST">
        <default key="_controller">Kematjaya\StateManagement\Controller\KmjLinkController:index</default>
    </route>
    
    <route id="kematjaya_link_create" path="/link/create" methods="GET POST">
        <default key="_controller">Kematjaya\StateManagement\Controller\KmjLinkController:create</default>
    </route>
    
    <route id="kematjaya_link_edit" path="/link/edit/{id}" methods="GET POST">
        <default key="_controller">Kematjaya\StateManagement\Controller\KmjLinkController:edit</default>
    </route>
    
    <route id="kematjaya_link_delete" path="/link/delete/{id}" methods="POST DELETE">
        <default key="_controller">Kematjaya\StateManagement\Controller\KmjLinkController:delete</default>
    </route>
    
</routes>
