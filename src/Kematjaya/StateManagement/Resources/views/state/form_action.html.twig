{% extends '@KmjState/base.html.twig' %}

{% block title %}{{title}}{% endblock %}

{% block body %}
    <div class="page-title">
        <h3 class="breadcrumb-header">{{title}}</h3>
    </div>
    <div id="main-wrapper">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="panel panel-white">
                    <div class="panel-heading clearfix">
                        <a href="{{path('kematjaya_state_action', {id: kmj_state.id})}}" class="btn btn-primary btn-sm pull-right"><span class="fa fa-list"></span> {{ 'back'|trans }}</a>
                        <h4 class="panel-title">{{title}}</h4>
                        
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive invoice-table">
                            
                            {{ include('@KmjState/_flashes.html.twig') }}
                            
                            {{ form_start(form) }}
                                {{ form_widget(form) }}
                                <button class="btn btn-success btn-sm"><span class="fa fa-save"></span> {{ 'save'|trans }}</button>
                                <a href="{{path('kematjaya_state_action', {id: kmj_state.id})}}" class="btn btn-primary btn-sm"><span class="fa fa-list"></span> {{ 'back'|trans }}</a>
                            {{ form_end(form) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}